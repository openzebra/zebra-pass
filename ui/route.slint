import { Button, VerticalBox } from "std-widgets.slint";

import { LockPage } from "./pages/lock.slint";

export enum Routers {
    Lock
}

export component Route inherits Window {
    preferred-width: 750pt;
    preferred-height: 450pt;

    in property<Routers> route: Routers.Lock;

    if(root.route == Routers.Lock) : VerticalBox {
        LockPage {}
    }
}
