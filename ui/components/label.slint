import { StyleMetrics } from "std-widgets.slint";

export component ZLabel inherits Rectangle {
    in property <string> text;

    height: 20pt;
    width: 50pt;
    border-radius: 4pt;
    border-color: root.background.darker(20%);
    border-width: 1pt;

    Text {
        font-weight: 900;
        color: root.background.darker(40%);
        text: text;
    }
}